<h1 class="time">Index of All Systems As Of: <%=Time.now%></h1>

<table class="grid" width="900" border="1">
    <th>Name</th>
    <th>FQDN</th>
    <th>Location</th>

    <% Instance.all.each do |system|%>
          <tr class='server' id="<%=system._id%>">
            <td width="200"><%= system.name%></td>
            <td width="200"><%= system.canonical%></td>
            <td width="200"><%= system.location%></td>
          </tr>
    <%end%>
</table>

