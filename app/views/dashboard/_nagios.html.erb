<h1> Started collecting data on: <%= Time.at(@nagios.timestamp)%>  </h1>

    <table width="800" border="1">
        <th>Name</th>
        <th>State</th>
        <th>Last Runtime</th>
        <th>Next Runtime</th>

      <%@nagios.each{|entry| %>
      <tr>
        <td><%= entry['host_name']%></td>
	<td><%= entry['service_description']%></td>
        <td <%= entry['current state'].nil? "class='background-color: red'" : "class='background-color: green'" %>> STATUS </td>
        <td><%= entry['plugin_output']%></td>
      </tr>  <%}%>
